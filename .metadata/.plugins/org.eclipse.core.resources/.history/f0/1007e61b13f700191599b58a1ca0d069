public class TestC {
	public static void main(String[] args) {
		System.out.println("Scanner class");
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enetr age");
		int age = sc.nextInt();
		System.out.println("Age is"+age);
		
	}

}
